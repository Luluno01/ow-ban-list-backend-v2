{
  "name": "ow-ban-list-backend",
  "version": "1.0.0",
  "description": "Overwatch ban list backend (standalone version, not for netlify)",
  "main": "build/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "set NODE_ENV=development && nodemon --watch 'src/**/*' -e ts,tsx --exec ts-node ./src/server.ts",
    "buildSDK": "cd src/vendor/ow-ban-list && npm install && npm run build",
    "buildAll": "npm run buildSDK && tsc",
    "build": "tsc",
    "serve": "set NODE_ENV=production && node ./build/server.js"
  },
  "author": "Untitled",
  "license": "ISC",
  "dependencies": {
    "koa": "^2.7.0",
    "koa-log4": "^2.3.2",
    "koa-router": "^7.4.0"
  },
  "devDependencies": {
    "@types/koa": "^2.0.48",
    "@types/koa-router": "^7.0.39",
    "nodemon": "^1.18.10",
    "ts-node": "^8.0.2",
    "typescript": "^3.3.3"
  }
}
